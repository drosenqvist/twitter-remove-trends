{
  "manifest_version": 2,

  "name": "Twitter Clean Up",
  "description": "This extension removes several elements of the Twitter webpage",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["script.js"]
    }
  ],

  "browser_action": {
   "default_icon": "icon.png"
  },

  "permissions": [
    "tabs", "*://twitter.com/*"
  ]
}
